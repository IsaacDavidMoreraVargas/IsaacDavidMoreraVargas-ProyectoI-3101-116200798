                            <div class="contenedor-datos">
                                <label class="titulo-valor color-padre">Nombre Clinica</label>
                                <input runat="server" asp-for="registro_clinica.Nombre_Clinica" class="input-valor border-hijo" id="" runat="server" value="" type="text" onkeyup="consulta_Obtener(1); check_Empty()" maxlength="300" required />
                            </div>
                            <div class="contenedor-datos">
                                <label class="titulo-valor color-padre">Telefono Administracion Clinica</label>
