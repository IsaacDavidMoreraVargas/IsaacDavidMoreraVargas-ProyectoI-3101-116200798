                        <div class="contenedor-fila border-padre">
                            <label class="contenedor-titulo color-titulo-padre">Datos Paciente</label>
                            <div class="contenedor-datos">
                                <label class="titulo-valor color-padre">Fecha Registro</label>
                                <input runat="server" asp-for="registro_paciente.Fecha_Registro" readonly class="input-date border-hijo" id="" runat="server" value="" type="text" onchange="check_Empty()" required />
