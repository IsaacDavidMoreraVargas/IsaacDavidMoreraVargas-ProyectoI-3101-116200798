                                <label class="titulo-valor color-padre">Estado/Provincia Paciente</label>
                                <select runat="server" asp-for="registro_paciente.Estado_Provincia_Paciente" class="tag-select color-select provincias" id="" runat="server" value="" onchange="check_Empty()" maxlength="300" required>
                                    <option value="---">---</option>
                                </select>
                            </div>
                            <div class="contenedor-datos">
                                <label class="titulo-valor color-padre">Distrito Paciente (Opcional)</label>
                                <select runat="server" asp-for="registro_paciente.Distrito_Paciente" class="tag-select color-select distrito" id="" runat="server" value="" onchange="check_Empty()" maxlength="300">
                                    <option value="---">---</option>
                                </select>
                            </div>
                            <div class="contenedor-datos">
                                <label class="titulo-valor color-padre">Genero Paciente</label>
                                <select runat="server" asp-for="registro_paciente.Genero_Paciente" class="tag-select color-select" id="" runat="server" value="" onchange="check_Empty()" maxlength="1" required>
                                    <option value="---">---</option>
                                    <option value="M">M</option>
                                    <option value="F">F</option>
                                </select>
                            </div>
                            <div class="contenedor-datos">
                                <label class="titulo-valor color-padre">Estado Civil Paciente</label>
                                <select runat="server" asp-for="registro_paciente.Estado_Civil_Paciente" class="tag-select color-select" id="" runat="server" value="" onchange="check_Empty()" maxlength="300" required>
                                    <option value="---">---</option>
                                    <option value="Soltero">Soltero</option>
                                    <option value="Casado">Casado</option>
                                    <option value="Union Libre">Union Libre</option>
                                    <option value="Viudo">Viudo</option>
                                </select>
                            </div>
                        </div>

                        <input class="button-to-submit" type="submit" value="Guardar" />
                    </form>
                </div>
                break;
            case 3:
      
            <!--inyecciones-->
